            </div>
        </div>
    </div>

    <script>
        // Content file loading functionality
        function loadContentFiles() {
            // This function would load content from .txt files
            // For now, it shows placeholder content
            console.log('Content files would be loaded here');
        }
        function openContentFile(filename) {
            alert(`In VS Code, you would open: content/${filename}\n\nThis is where you'll edit your content. Use GitHub Copilot to help with writing and organization!`);
        }

        // Study Dropdown Functionality
        function toggleStudyDropdown(studyId) {
            const dropdown = document.getElementById(studyId + '-dropdown');
            const card = dropdown.closest('.study-card');
            const arrow = card.querySelector('.expand-arrow');
            
            if (dropdown.classList.contains('expanded')) {
                // Close dropdown
                dropdown.classList.remove('expanded');
                card.classList.remove('expanded');
                arrow.textContent = '🔽';
            } else {
                // Close all other dropdowns first
                document.querySelectorAll('.study-dropdown.expanded').forEach(otherDropdown => {
                    otherDropdown.classList.remove('expanded');
                    otherDropdown.closest('.study-card').classList.remove('expanded');
                    otherDropdown.closest('.study-card').querySelector('.expand-arrow').textContent = '🔽';
                });
                
                // Open this dropdown
                dropdown.classList.add('expanded');
                card.classList.add('expanded');
                arrow.textContent = '🔼';
                
                // Smooth scroll to make sure content is visible
                setTimeout(() => {
                    dropdown.scrollIntoView({ 
                        behavior: 'smooth', 
                        block: 'nearest' 
                    });
                }, 100);
            }
        }

        // Tab Navigation Functionality
        function showTab(tabNumber) {
            // Hide all tab panels
            const panels = document.querySelectorAll('.tab-panel');
            panels.forEach(panel => panel.classList.remove('active'));
            
            // Remove active class from all tab buttons
            const buttons = document.querySelectorAll('.tab-button');
            buttons.forEach(button => button.classList.remove('active'));
            
            // Show the selected tab panel
            document.getElementById(`tab-${tabNumber}`).classList.add('active');
            
            // Add active class to the clicked button
            buttons[tabNumber - 1].classList.add('active');
            
            // Update progress indicator
            updateProgressIndicator(tabNumber);
        }

        // Problem Definition Framework Tab Functionality
        function showFrameworkTab(tabName) {
            // Hide all framework content
            document.getElementById('xy-content').style.display = 'none';
            document.getElementById('picoc-content').style.display = 'none';
            document.getElementById('details-content').style.display = 'none';
            
            // Reset all framework tab buttons
            document.getElementById('xy-tab').style.background = '#B8E0D2';
            document.getElementById('xy-tab').style.color = '#0A4A54';
            document.getElementById('picoc-tab').style.background = '#B8E0D2';
            document.getElementById('picoc-tab').style.color = '#0A4A54';
            document.getElementById('details-tab').style.background = '#B8E0D2';
            document.getElementById('details-tab').style.color = '#0A4A54';
            
            // Show selected content and activate button
            document.getElementById(tabName + '-content').style.display = 'block';
            document.getElementById(tabName + '-tab').style.background = '#005F73';
            document.getElementById(tabName + '-tab').style.color = 'white';
        }

        // Toggle collapsible sections
        function toggleSection(sectionId) {
            const content = document.getElementById(sectionId + '-content');
            const icon = document.getElementById(sectionId + '-icon');
            
            if (content.style.display === 'none' || content.style.display === '') {
                content.style.display = 'block';
                icon.textContent = '▲';
            } else {
                content.style.display = 'none';
                icon.textContent = '▼';
            }
        }
        
        // Update Progress Indicator based on current tab
        function updateProgressIndicator(currentTab) {
            const progressSteps = document.querySelectorAll('.progress-step');
            
            progressSteps.forEach((step, index) => {
                step.classList.remove('completed', 'active', 'pending');
                
                if (index + 1 < currentTab) {
                    step.classList.add('completed');
                } else if (index + 1 === currentTab) {
                    step.classList.add('active');
                } else {
                    step.classList.add('pending');
                }
            });
        }

        // Stakeholder Analysis Functions
        function showStakeholderTab(tab) {
            // Hide all content sections
            const contentIds = ['stakeholder-overview-content', 'stakeholder-analysis-content', 'stakeholder-engagement-content', 'stakeholder-evidence-content'];
            contentIds.forEach(id => {
                const element = document.getElementById(id);
                if (element) element.style.display = 'none';
            });
            
            // Remove active class from all tabs
            document.querySelectorAll('.stakeholder-tab').forEach(t => {
                t.style.background = '#B8E0D2';
                t.style.color = '#0A4A54';
            });
            
            // Show selected content and activate tab
            const selectedContent = document.getElementById('stakeholder-' + tab + '-content');
            if (selectedContent) selectedContent.style.display = 'block';
            
            const selectedTab = document.getElementById('stakeholder-' + tab + '-tab');
            if (selectedTab) {
                selectedTab.style.background = '#005F73';
                selectedTab.style.color = 'white';
            }
        }

        // Toggle individual stakeholder sections
        function toggleStakeholderSection(sectionId) {
            const details = document.getElementById(sectionId + '-details');
            const arrow = document.getElementById(sectionId + '-arrow');
            
            if (details.style.display === 'none') {
                details.style.display = 'block';
                arrow.style.transform = 'rotate(180deg)';
            } else {
                details.style.display = 'none';
                arrow.style.transform = 'rotate(0deg)';
            }
        }

        // Toggle engagement strategy sections
        function toggleEngagementSection(sectionId) {
            const details = document.getElementById(sectionId + '-details');
            const arrow = document.getElementById(sectionId + '-arrow');
            
            if (details.style.display === 'none') {
                details.style.display = 'block';
                arrow.style.transform = 'rotate(180deg)';
            } else {
                details.style.display = 'none';
                arrow.style.transform = 'rotate(0deg)';
            }
        }

        // Toggle interview sections in Evidence Collection tab
        function toggleInterviewSection(sectionId) {
            const details = document.getElementById(sectionId + '-details');
            const arrow = document.getElementById(sectionId + '-arrow');
            
            if (details.style.display === 'none') {
                details.style.display = 'block';
                arrow.style.transform = 'rotate(180deg)';
            } else {
                details.style.display = 'none';
                arrow.style.transform = 'rotate(0deg)';
            }
        }

        function toggleStakeholderDetails(stakeholderId) {
            const details = document.getElementById(stakeholderId + '-details');
            const toggle = document.getElementById(stakeholderId + '-toggle');
            
            if (details.style.display === 'none' || details.style.display === '') {
                details.style.display = 'block';
                toggle.textContent = '📊 Hide Analysis';
            } else {
                details.style.display = 'none';
                toggle.textContent = '📊 View Analysis';
            }
        }

        function filterStakeholders() {
            const filter = document.getElementById('stakeholder-filter').value;
            const categories = document.querySelectorAll('.stakeholder-category');
            
            // Show all categories first
            categories.forEach(category => {
                category.style.display = 'block';
            });
            
            // Apply filter to main category boxes only
            if (filter === 'all') {
                // Show all categories
                return;
            } else if (filter === 'internal') {
                // Hide external stakeholders category box
                document.querySelector('[data-type="external"]').style.display = 'none';
            } else if (filter === 'external') {
                // Hide internal stakeholders category box
                document.querySelector('[data-type="internal"]').style.display = 'none';
            } else if (filter === 'high-influence') {
                // Show all categories but highlight that we're focusing on high-influence
                // All detailed cards remain visible within their categories
                return;
            } else if (filter === 'supporters') {
                // Show all categories but highlight that we're focusing on supporters
                // All detailed cards remain visible within their categories
                return;
            }
        }

        // Enhanced toggleSection function for stakeholder sections
        function toggleSection(sectionId) {
            const content = document.getElementById(sectionId + '-content');
            const arrow = document.getElementById(sectionId + '-arrow');
            
            if (content.style.display === 'none' || content.style.display === '') {
                content.style.display = 'block';
                arrow.style.transform = 'rotate(180deg)';
            } else {
                content.style.display = 'none';
                arrow.style.transform = 'rotate(0deg)';
            }
        }



        function updateSourceTarget(value) {
            document.getElementById('source-value').textContent = value + ' Sources';
        }

        function updateConfidenceTarget(value) {
            document.getElementById('confidence-value').textContent = value + '% Confident';
        }

        function updateTimelineTarget() {
            // This function is called when timeline select changes
        }

        function generateCriteriaSummary() {
            const sources = document.getElementById('source-slider').value;
            const confidence = document.getElementById('confidence-slider').value;
            const timeline = document.getElementById('timeline-select').value;
            
            const summary = `
                <strong>📚 Evidence Target:</strong> ${sources} peer-reviewed journal articles<br>
                <strong>👨‍💼 Manager Confidence:</strong> ${confidence}% of managers confident in recommendations<br>
                <strong>⏰ Timeline Goal:</strong> Complete research within ${timeline} months<br>
                <strong>📊 Success Threshold:</strong> ${sources >= 25 ? 'Target Performance' : sources >= 20 ? 'Minimum Acceptable' : 'Below Minimum'}
            `;
            
            document.getElementById('criteria-summary').style.display = 'block';
            document.getElementById('summary-content').innerHTML = summary;
        }

        function toggleMilestone(phase) {
            milestoneProgress[phase] = (milestoneProgress[phase] + 1) % 3;
            const element = document.getElementById(phase + '-milestone');
            const statusElement = document.getElementById(phase + '-status');
            
            const statuses = ['NOT STARTED', 'IN PROGRESS', 'COMPLETE'];
            const colors = [
                { bg: '#FEE2E2', text: '#DC2626' },
                { bg: '#FEF3C7', text: '#F59E0B' },
                { bg: '#DCFCE7', text: '#16A34A' }
            ];
            
            const currentStatus = milestoneProgress[phase];
            statusElement.textContent = statuses[currentStatus];
            statusElement.style.background = colors[currentStatus].bg;
            statusElement.style.color = colors[currentStatus].text;
            
            updateTimelineProgress();
        }

        function updateTimelineProgress() {
            const startedPhases = Object.values(milestoneProgress).filter(status => status > 0).length;
            const totalPhases = Object.keys(milestoneProgress).length;
            const percentage = (startedPhases / totalPhases) * 100;
            
            document.getElementById('timeline-bar').style.width = percentage + '%';
            document.getElementById('timeline-progress').textContent = `${startedPhases}/${totalPhases} Phases Started`;
        }

        function toggleRiskAlert(riskType) {
            riskLevels[riskType] = (riskLevels[riskType] + 1) % 3;
            const indicator = document.getElementById(riskType + '-indicator');
            
            const levels = ['LOW', 'MEDIUM', 'HIGH'];
            const colors = ['#10B981', '#F59E0B', '#DC2626'];
            
            const currentLevel = riskLevels[riskType];
            indicator.textContent = levels[currentLevel];
            indicator.style.background = colors[currentLevel];
        }



        function exportCriteriaData() {
            const data = {
                criteriaProgress,
                milestoneProgress,
                riskLevels,
                targets: {
                    sources: document.getElementById('source-slider').value,
                    confidence: document.getElementById('confidence-slider').value,
                    timeline: document.getElementById('timeline-select').value
                },
                exportDate: new Date().toISOString()
            };
            
            const dataStr = JSON.stringify(data, null, 2);
            alert(`Success Criteria Progress Data:\n\n${dataStr}\n\nThis data shows your current progress and settings. You could save this to track your research project status.`);
        }

        function toggleSuccessCriteriaHelp() {
            const helpSection = document.getElementById('success-criteria-help');
            const button = document.getElementById('success-help-btn');
            
            if (helpSection.style.display === 'none' || helpSection.style.display === '') {
                helpSection.style.display = 'block';
                button.textContent = '🔼 Hide Tips & Examples';
                button.style.background = '#DC2626';
            } else {
                helpSection.style.display = 'none';
                button.textContent = '💡 Show Tips & Examples';
                button.style.background = '#059669';
            }
        }

        // Success Criteria Section Toggle Functions
        function toggleSuccessSection(sectionId) {
            const content = document.getElementById(sectionId + '-content');
            const arrow = document.getElementById(sectionId + '-arrow');
            
            if (content.style.display === 'none') {
                content.style.display = 'block';
                arrow.textContent = '🔼';
                arrow.style.transform = 'rotate(180deg)';
            } else {
                content.style.display = 'none';
                arrow.textContent = '🔽';
                arrow.style.transform = 'rotate(0deg)';
            }
        }

        function togglePhaseDetails(phaseId) {
            const details = document.getElementById(phaseId + '-details');
            const arrow = document.getElementById(phaseId + '-arrow');
            
            if (details.style.display === 'none') {
                details.style.display = 'block';
                arrow.textContent = '🔼';
                arrow.style.transform = 'rotate(180deg)';
            } else {
                details.style.display = 'none';
                arrow.textContent = '🔽';
                arrow.style.transform = 'rotate(0deg)';
            }
        }

        // Initialize dashboard
        document.addEventListener('DOMContentLoaded', function() {
            loadContentFiles();
            updateProgressIndicator(1);
        });
    </script>
